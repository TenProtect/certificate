
/* 全局CSS变量定义 */
.data-v-2c1dbf3d:root {
  --status-bar-height: 44px;
}

/* 支付宝小程序环境变量支持 */
@supports (top: env(safe-area-inset-top)) {
.data-v-2c1dbf3d:root {
    --status-bar-height: env(safe-area-inset-top);
}
}

/* 微信小程序环境变量支持 */
@supports (top: constant(safe-area-inset-top)) {
.data-v-2c1dbf3d:root {
    --status-bar-height: constant(safe-area-inset-top);
}
}
.order-page.data-v-2c1dbf3d {
  background-color: #f5f5f5;
  min-height: 100%;
  box-sizing: border-box;
  overflow-y: hidden;
}
.page-title.data-v-2c1dbf3d {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}
.page-header.data-v-2c1dbf3d {
  background: white;
  padding: 0 20rpx 30rpx;
  text-align: center;
  position: relative;
  /* CSS环境变量回退方案 */
  padding-top: var(--status-bar-height, 44px);
}

/* 针对特殊机型的适配 */
@media screen and (device-width: 414px) and (device-height: 896px) {
  /* iPhone XR, iPhone 11 */
.page-header.data-v-2c1dbf3d {
    padding-top: 44px;
}
}
@media screen and (device-width: 375px) and (device-height: 812px) {
  /* iPhone X, iPhone XS, iPhone 11 Pro */
.page-header.data-v-2c1dbf3d {
    padding-top: 44px;
}
}
@media screen and (device-width: 414px) and (device-height: 896px) {
  /* iPhone XS Max, iPhone 11 Pro Max */
.page-header.data-v-2c1dbf3d {
    padding-top: 44px;
}
}
.order-tabs.data-v-2c1dbf3d {
  background: white;
  display: flex;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #e9ecef;
}
.tab-item.data-v-2c1dbf3d {
  flex: 1;
  text-align: center;
  padding: 30rpx 0;
  position: relative;
  transition: all 0.2s ease;
}
.tab-item.data-v-2c1dbf3d:active {
  background: rgba(61, 69, 230, 0.05);
}
.tab-item.active .tab-text.data-v-2c1dbf3d {
  color: #3d45e6;
  font-weight: 500;
}
.tab-item.active.data-v-2c1dbf3d::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 60rpx;
  height: 4rpx;
  background: #3d45e6;
  border-radius: 2rpx;
}
.tab-text.data-v-2c1dbf3d {
  font-size: 28rpx;
  color: #666;
  transition: color 0.2s ease;
}
.order-list.data-v-2c1dbf3d {
  padding: 0 20rpx 40rpx;
}

/* 全局温馨提示样式 */
.global-warm-tip.data-v-2c1dbf3d {
  display: flex;
  align-items: center;
  margin: 20rpx 0 30rpx;
  padding: 16rpx 20rpx;
  background: linear-gradient(135deg, #fff7e6 0%, #ffecc7 100%);
  border-radius: 16rpx;
  border: 1rpx solid #ffd591;
  box-shadow: 0 2rpx 8rpx rgba(255, 193, 7, 0.1);
}
.global-warm-tip .tip-icon.data-v-2c1dbf3d {
  font-size: 28rpx;
  margin-right: 12rpx;
  animation: pulse-2c1dbf3d 2s infinite;
}
@keyframes pulse-2c1dbf3d {
0%, 100% { transform: scale(1);
}
50% { transform: scale(1.1);
}
}
.global-warm-tip .tip-text.data-v-2c1dbf3d {
  font-size: 26rpx;
  color: #d48806;
  line-height: 1.4;
  flex: 1;
  font-weight: 500;
}
