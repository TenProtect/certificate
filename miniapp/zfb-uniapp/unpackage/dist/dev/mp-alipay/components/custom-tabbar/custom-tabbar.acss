
.custom-tabbar.data-v-51c48e3c {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1000;
  background: rgba(255, 255, 255, 0.95);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
}
.tabbar-blur-bg.data-v-51c48e3c {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(180deg, 
    rgba(255, 255, 255, 0.9) 0%, 
    rgba(255, 255, 255, 0.95) 50%, 
    rgba(255, 255, 255, 1) 100%
  );
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
}
.tabbar-border.data-v-51c48e3c {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 1rpx;
  background: linear-gradient(90deg, 
    transparent 0%, 
    rgba(61, 69, 230, 0.1) 50%, 
    transparent 100%
  );
}
.tabbar-content.data-v-51c48e3c {
  position: relative;
  display: flex;
  height: 120rpx;
  align-items: center;
  justify-content: space-around;
  padding: 0 20rpx;
}
.tab-item.data-v-51c48e3c {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
  padding: 10rpx 0;
  transition: all 0.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}
.tab-glow.data-v-51c48e3c {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 90rpx;
  height: 90rpx;
  background: radial-gradient(circle, rgba(61, 69, 230, 0.15) 0%, transparent 70%);
  border-radius: 50%;
  animation: glow-pulse-51c48e3c 2s ease-in-out infinite;
}
@keyframes glow-pulse-51c48e3c {
0%, 100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 0.5;
}
50% {
    transform: translate(-50%, -50%) scale(1.2);
    opacity: 0.8;
}
}
.tab-icon-wrapper.data-v-51c48e3c {
  position: relative;
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 8rpx;
}
.tab-svg-icon.data-v-51c48e3c {
  width: 100%;
  height: 100%;
  position: relative;
  transition: all 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}
.tab-svg-icon.active.data-v-51c48e3c {
  animation: icon-bounce-51c48e3c 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

/* 首页图标样式 */
.svg-home.data-v-51c48e3c {
  position: relative;
  width: 100%;
  height: 100%;
}
.home-base.data-v-51c48e3c {
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 38rpx;
  height: 24rpx;
  background: #999;
  transition: all 0.3s ease;
}
.tab-svg-icon.active .home-base.data-v-51c48e3c {
  background: #3d45e6;
}
.home-roof.data-v-51c48e3c {
  position: absolute;
  top: 8rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 0;
  height: 0;
  border-left: 24rpx solid transparent;
  border-right: 24rpx solid transparent;
  border-bottom: 20rpx solid #999;
  transition: all 0.3s ease;
}
.tab-svg-icon.active .home-roof.data-v-51c48e3c {
  border-bottom-color: #3d45e6;
}
.home-door.data-v-51c48e3c {
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 10rpx;
  height: 14rpx;
  background: white;
  border-radius: 1rpx;
}

/* 订单图标样式 */
.svg-order.data-v-51c48e3c {
  position: relative;
  width: 100%;
  height: 100%;
}
.order-paper.data-v-51c48e3c {
  position: absolute;
  top: 6rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 36rpx;
  height: 46rpx;
  background: #999;
  border-radius: 4rpx;
  transition: all 0.3s ease;
}
.tab-svg-icon.active .order-paper.data-v-51c48e3c {
  background: #3d45e6;
}
.order-lines.data-v-51c48e3c {
  position: absolute;
  top: 16rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 24rpx;
  height: 2rpx;
  background: white;
  box-shadow: 0 8rpx 0 white, 0 16rpx 0 white;
}
.order-check.data-v-51c48e3c {
  position: absolute;
  top: 38rpx;
  right: 6rpx;
  width: 10rpx;
  height: 10rpx;
  background: #4CAF50;
  border-radius: 50%;
  animation: check-appear-51c48e3c 0.3s ease-out;
}
@keyframes check-appear-51c48e3c {
0% {
    transform: scale(0);
    opacity: 0;
}
100% {
    transform: scale(1);
    opacity: 1;
}
}

/* 我的图标样式 */
.svg-profile.data-v-51c48e3c {
  position: relative;
  width: 100%;
  height: 100%;
}
.profile-head.data-v-51c48e3c {
  position: absolute;
  top: 8rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 22rpx;
  height: 22rpx;
  background: #999;
  border-radius: 50%;
  transition: all 0.3s ease;
}
.tab-svg-icon.active .profile-head.data-v-51c48e3c {
  background: #3d45e6;
}
.profile-body.data-v-51c48e3c {
  position: absolute;
  bottom: 8rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 38rpx;
  height: 24rpx;
  background: #999;
  border-radius: 19rpx 19rpx 0 0;
  transition: all 0.3s ease;
}
.tab-svg-icon.active .profile-body.data-v-51c48e3c {
  background: #3d45e6;
}
@keyframes icon-bounce-51c48e3c {
0% {
    transform: scale(1);
}
50% {
    transform: scale(1.2) translateY(-4rpx);
}
100% {
    transform: scale(1);
}
}
.icon-dot.data-v-51c48e3c {
  position: absolute;
  top: -6rpx;
  right: -6rpx;
  width: 16rpx;
  height: 16rpx;
  background: linear-gradient(135deg, #3d45e6 0%, #5b63f5 100%);
  border-radius: 50%;
  border: 2rpx solid white;
  animation: dot-pulse-51c48e3c 1.5s ease-in-out infinite;
}
@keyframes dot-pulse-51c48e3c {
0%, 100% {
    transform: scale(1);
    box-shadow: 0 0 0 0 rgba(61, 69, 230, 0.7);
}
50% {
    transform: scale(1.1);
    box-shadow: 0 0 0 8rpx rgba(61, 69, 230, 0);
}
}
.tab-text.data-v-51c48e3c {
  font-size: 22rpx;
  color: #999;
  font-weight: 400;
  transition: all 0.3s ease;
  letter-spacing: 0.5rpx;
}
.tab-text.active.data-v-51c48e3c {
  color: #3d45e6;
  font-weight: 600;
  transform: scale(1.05);
}
.active-indicator.data-v-51c48e3c {
  position: absolute;
  bottom: -2rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 30rpx;
  height: 6rpx;
  background: linear-gradient(90deg, #3d45e6 0%, #5b63f5 100%);
  border-radius: 3rpx;
  animation: indicator-slide-in-51c48e3c 0.3s ease-out;
}
@keyframes indicator-slide-in-51c48e3c {
0% {
    width: 0;
    opacity: 0;
}
100% {
    width: 30rpx;
    opacity: 1;
}
}

/* 深色模式支持 */
@media (prefers-color-scheme: dark) {
.custom-tabbar.data-v-51c48e3c {
    background: rgba(28, 28, 30, 0.95);
}
.tabbar-blur-bg.data-v-51c48e3c {
    background: linear-gradient(180deg, 
      rgba(28, 28, 30, 0.9) 0%, 
      rgba(28, 28, 30, 0.95) 50%, 
      rgba(28, 28, 30, 1) 100%
    );
}
.tab-text.data-v-51c48e3c {
    color: #8e8e93;
}
.tab-text.active.data-v-51c48e3c {
    color: #007aff;
}
}

/* 支付宝小程序适配 */
.custom-tabbar.data-v-51c48e3c {
  box-shadow: 0 -2rpx 20rpx rgba(0, 0, 0, 0.1);
  /* 硬件加速 */
  transform: translateZ(0);
  -webkit-transform: translateZ(0);
  /* 防止在某些设备上出现白线 */
  border-top: 1rpx solid rgba(0, 0, 0, 0.05);
}

/* 优化毛玻璃效果的兼容性 */
@supports not ((-webkit-backdrop-filter: blur(20px)) or (backdrop-filter: blur(20px))) {
.custom-tabbar.data-v-51c48e3c,
  .tabbar-blur-bg.data-v-51c48e3c {
    background: rgba(255, 255, 255, 0.98);
}
}

/* 针对支付宝小程序的特殊优化 */
.custom-tabbar.data-v-51c48e3c {
  background: rgba(255, 255, 255, 0.98);
  backdrop-filter: none;
  -webkit-backdrop-filter: none;
}
.tabbar-blur-bg.data-v-51c48e3c {
  background: linear-gradient(180deg, 
    rgba(255, 255, 255, 0.95) 0%, 
    rgba(255, 255, 255, 1) 100%
  );
  backdrop-filter: none;
  -webkit-backdrop-filter: none;
}


/* 确保在所有设备上都有良好的性能 */
.tab-item.data-v-51c48e3c,
.tab-svg-icon.data-v-51c48e3c,
.tab-text.data-v-51c48e3c {
  will-change: transform, color;
}
